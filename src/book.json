{
  "gitbook": "^2.3.0",
  "title": "GeoServer in action",
  "description": "GeoServer in action workshop materials",
  "language": "de",
  "variables": {
    "workshopName": "GeoServer in action",
    "workshopRawSourceBaseUrl": "https://rawgit.com/terrestris/geoserver-in-action-ws/master/src/",
    "workshopDownloadUrl": "https://terrestris.github.io/geoserver-in-action-ws/geoserver-in-action-ws.zip",
    "workshopPdfUrl": "https://terrestris.github.io/geoserver-in-action-ws/geoserver-in-action-ws.pdf",
    "workshopEpubUrl": "https://terrestris.github.io/geoserver-in-action-ws/geoserver-in-action-ws.epub",
    "workshopMaterialsDownloadUrl": "https://terrestris.github.io/geoserver-in-action-ws/materials.tar.gz",
    "geoServerPhysicalPath": "/usr/local/lib/geoserver-2.15.1/",
    "geoServerBaseUrl": "http://localhost:8082/geoserver",
    "geoServerUser": "admin",
    "geoServerPassword": "geoserver",
    "geoServerVersion": "2.15.1",
    "osGeoLiveVersion": "13.0",
    "authors": [{
      "name": "Nils Bühner",
      "mail": "buehner@terrestris.de"
    }, {
      "name": "André Henn",
      "mail": "henn@terrestris.de"
    }, {
      "name": "Daniel Koch",
      "mail": "koch@terrestris.de"
    }]
  },
  "plugins": [
    "advanced-emoji",
    "edit-link",
    "expandable-chapters",
    "image-captions",
    "richquotes",
    "sectionx"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/terrestris/geoserver-in-action-ws/edit/master/src",
      "label": "Edit…"
    },
    "image-captions": {
      "caption": "_CAPTION_"
    },
    "richquotes": {
      "task": {
        "alert": "info",
        "picto": "fa-file-code-o"
      },
      "exercise": {
        "alert": "info",
        "picto": "fa-file-code-o"
      }
    }
  }
}
